<?xml version="1.0" ?>
<launch>
    <node name="depthimage_to_laserscan" pkg="depthimage_to_laserscan" type="depthimage_to_laserscan" >
        <remap from="image" to="camera/depth/image_rect_raw"/>
        <remap from="camera_info" to="camera/depth/camera_info"/>
        <rosparam command="load" file="$(find robotont_laserscan_to_distance)/config/laser_params.yaml" />
    </node>
    <node pkg="robotont_laserscan_to_distance" name="laserscan_to_distance" type="laserscan_to_distance.py" output="screen" />
</launch>
